    "types": "./dist/index.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "build": "tsup",
    "build-notypes": "bun build ./src/index.ts --outdir ./dist",
    "prepublishOnly": "bun run build"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "socket.io-client": "^4.8.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  },
  "devDependencies": {
    "@swc/core": "^1.11.18",
    "@types/bun": "latest",
    "@types/node-wav": "^0.0.3",
    "@vitest/browser": "^3.0.5",
    "eslint": "^9.8.0",
    "microbundle": "^0.15.1",
    "mock-socket": "^9.3.1",
    "node-wav": "^0.0.2",
    "playwright": "^1.46.1",
    "vitest": "^3.0.5"
  }
}
